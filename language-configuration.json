{
   
      "brackets": [["[", "]"], ["(", ")"]],
      "autoClosingPairs": [
        { "open": "[", "close": "]"},
        { "open": "{", "close": "}" },
        { "open": "\"", "close": "\""},
        { "open": "(", "close": ")" }
      ],
      "autoCloseBefore": ";\n\t",
      "surroundingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["'", "'"],
        ["\"", "\""]
      ],
      "comments": {
        "lineComment": "",
        "blockComment": ["{", "}"]
        },
        "folding": {
          "markers": {
            "start": "^{region\\b",
            "end": "^{endregion\\b"
          }
        },
        "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)",
        "indentationRules": {
          "increaseIndentPattern": "^((?!.*?\\/\\*).*\\*\/)?\\s*[\\}\\]].*$",
          "decreaseIndentPattern": "^((?!\\/\\/).)*(\\{[^}\"'`]*|\\([^)\"'`]*|\\[[^\\]\"'`]*)$"
        }
}